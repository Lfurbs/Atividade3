#include<stdio.h>


main(){
 
    int a=0, b=1212;

    printf("Informe seu salario");
    scanf("%d", &a);

    if ( a >= b)
    {
        printf("Voce tem um salario superior, ou igual, ao salario minimo");
    }else if (a < b)
    {
        printf("Seu salario e inferior ao salario minimo");
    }
}
